<h1>Bot</h1>
<table width="100%">
  <thead>
    <th>Currency</th>
    <th>ID</th>
    <th>Last</th>
    <th>Lowest Ask</th>
    <th>Highest Bid</th>
    <th>Percent Change</th>
    <th>Base Volume</th>
    <th>Quote Volume</th>
    <th>Frozen?</th>
    <th>24 Hour High</th>
    <th>24 Hour Low</th>
  </thead>
  <tbody>
<% @data.each do |data| %>
  <tr>
  <td><%= data.currency %></td>
  <td><%= data.id %></td>
  <td><%= data.last %></td>
  <td><%= data.lowest_ask %></td>
  <td><%= data.highest_bid %></td>
  <td><%= data.percent_change %></td>
  <td><%= data.base_volume %></td>
  <td><%= data.quote_volume %></td>
  <td><%= data.is_frozen %></td>
  <td><%= data.high_24_hr %></td>
  <td><%= data.low_24_hr %></td>
  </tr>
<% end %>
  </tbody>
</table>
